{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "start:dev": "ts-node src/main/server.ts",
    "start:prod": "node dist/main/server.js",
    "watch": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "up": "sudo docker compose up",
    "down": "sudo docker compose down"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "git-commit-msg-linter": "^4.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.8.3",
    "typescript": "*"
  }
}
